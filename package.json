{
  "name": "esbuild-plugin-css-modules",
  "version": "0.1.0",
  "description": "Plugin for esbuild with partial support of css-modules",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "test": "npx ts-node node_modules/.bin/tape 'tests/*.tests.ts' | npx tap-spec"
  },
  "author": {
    "name": "Nikolai Mavrenkov",
    "email": "koluch@koluch.ru",
    "url": "koluch.ru"
  },
  "license": "ISC",
  "keywords": [
    "esbuild",
    "plugin",
    "css-modules"
  ],
  "dependencies": {
    "@types/css-tree": "^1.0.5",
    "@types/fs-extra": "^9.0.11",
    "@types/tmp": "^0.2.0",
    "css-tree": "^1.1.2",
    "fs-extra": "^9.0.1",
    "rm": "^0.1.8",
    "tmp": "^0.2.1",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "esbuild": "^0.8.32"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/tape": "^4.13.0",
    "esbuild": "^0.8.32",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.2.2",
    "ts-node": "^9.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npx prettier --write"
    ]
  }
}
